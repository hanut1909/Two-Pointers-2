class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        i=0
        count=0
        for j in range(len(nums)):
            if(j>0 and nums[j]==nums[j-1]):
                count=count+1
            else:
                count=1
            if(count<=2):
                nums[i]=nums[j]
                i=i+1
        return i
